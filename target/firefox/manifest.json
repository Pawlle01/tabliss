{
  "manifest_version": 2,
  "name": "Tabliss, Pawle",
  "short_name": "Tabliss",
  "version": "2.7.0",
  "description": "A beautiful New Tab page with many customisable backgrounds and widgets that does not require any permissions. Now with video slideshow capabilities!",
  "applications": {
    "gecko": {
      "id": "galler.business@gmail.com",
      "strict_min_version": "58.0"
    }
  },
  "chrome_settings_overrides": {
    "homepage": "index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  },
  "permissions": ["storage"]
}